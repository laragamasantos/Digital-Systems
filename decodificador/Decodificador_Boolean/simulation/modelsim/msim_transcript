wave create -driver freeze -pattern clock -initialvalue 0 -period 100ps -dutycycle 50 -starttime 0ps -endtime 10000ps sim:/Decodificador_Boolean/A
wave create -driver freeze -pattern clock -initialvalue 0 -period 80ps -dutycycle 50 -starttime 0ps -endtime 10000ps sim:/Decodificador_Boolean/B
wave create -driver freeze -pattern clock -initialvalue HiZ -period 50ps -dutycycle 50 -starttime 0ps -endtime 10000ps sim:/Decodificador_Boolean/C
wave create -driver freeze -pattern clock -initialvalue HiZ -period 25ps -dutycycle 50 -starttime 0ps -endtime 10000ps sim:/Decodificador_Boolean/D
add wave -position insertpoint  \
sim:/Decodificador_Boolean/a
add wave -position insertpoint  \
sim:/Decodificador_Boolean/b
add wave -position insertpoint  \
sim:/Decodificador_Boolean/c
add wave -position insertpoint  \
sim:/Decodificador_Boolean/d
add wave -position insertpoint  \
sim:/Decodificador_Boolean/e
add wave -position insertpoint  \
sim:/Decodificador_Boolean/f
add wave -position insertpoint  \
sim:/Decodificador_Boolean/g
