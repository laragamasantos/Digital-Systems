wave create -driver freeze -pattern clock -initialvalue 1 -period 33ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/mux_41/B
add wave -position insertpoint  \
sim:/mux_41/S0
add wave -position insertpoint  \
sim:/mux_41/S1
add wave -position insertpoint  \
sim:/mux_41/S
